}
.renter-info li span:nth-child(1){
	color: #c0c0c0;
}
.public-button-x {
	position: fixed;
	width: 100%;
	height:50px;
	bottom:0;
	left:0;
	background: #FFFFFF;
}
.public-button-x .button {
	width:98%;
	margin:0 auto;
	display: block;
	background:rgb(37, 155, 36);
	color: #FFFFFF;
	height:45px;
