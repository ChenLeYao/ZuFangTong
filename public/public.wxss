	overflow: scroll;
}

.swiper-slide input[type="file"]{
	display: none;
}
.swiper-slide label {
	display: block;
	height: 100%;
}
.edit {
	position: absolute;
	left:5px;
	bottom: 5px;
	z-index:100;
	color: #FFFFFF;
	background:rgba( 0, 0, 0 ,0.8);
	font-size: 12px;
	padding: 1px 4px;
	border-radius:20px;
	height:20px;
	line-height:20px;
}
.house-rentitem label{
	 display: inline-flex;
	min-width: 80px;
	 min-height:15px;
	 
}
.house-rentitem input[type="checkbox"] {
	display: none;
}
