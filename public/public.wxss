}
.house-detailpart{
	padding-bottom: 10px;
	 border-bottom: 1px solid #efefef;
   font-size: 14px;
  margin-top: 10px;

}
.house-detailpart span{
  margin-right: 5px;
}
.house-detailpart p {
    font-size: 13px;
	letter-spacing: 1px;
    color: #9f9f9f;
	margin:5px 0;
}
.house-detail h2 span i{
	color: #9f9f9f;
