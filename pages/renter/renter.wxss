/* pages/renter/renter.wxss */
@import '../../public/public.wxss';

.owner-vertical {
  height: 50px;
  font-size: 13px;
  width: 96%;
  margin:0 auto ;
 margin-bottom: -10px;
}
.owner-vertical span {
  float: left;
  height:30px;
  line-height:30px;
}
.owner-vertical span:nth-child(1){
    background-color: #4575d4;
    width: 15%;
    text-align: center;
    font-weight:600;
    color:#ffffff;
}

.owner-vertical span:nth-child(2){
      background: #333;
    color: #f9f9f9;
    border-color: #3498db;
    width: 85%;
    overflow: hidden;
    padding-left: 5px;
    font-size: 13px;
    box-sizing: border-box;
}